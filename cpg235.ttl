    skos:broader cpg235:0320 ;
    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 37" ;
    skos:prefLabel "Formal retention strategy" .

cpg235:0331 a skos:Concept ;
    skos:broader cpg235:0320 ;
    skos:definition "These controls would typically include appropriate segregation of duties, to reduce the potential for the actions of an individual to compromise data quality." ;
    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 38" ;
    skos:prefLabel "Protocols for data correction" .

cpg235:0405 a skos:Concept ;
