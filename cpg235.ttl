    skos:prefLabel "Retention controls" .

cpg235:0341 a skos:Concept ;
    skos:broader cpg235:0340 ;
    skos:definition "Data publication refers to the production of information for internal and external stakeholders (e.g. operational information, management information, customer information, media releases, regulatory reporting)." ;
    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 39" ;
    skos:prefLabel "Controls to ensure that published data meets the understood content and quality requirements of users" .

cpg235:0360 a skos:Concept ;
    skos:broader cpg235:0300 ;
    skos:inScheme cpg235:scheme ;
    skos:prefLabel "Disposal controls" .

cpg235:0363 a skos:Concept ;
    skos:broader cpg235:0360 ;
    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 42" ;
    skos:prefLabel "Examples of disposal controls" .

cpg235:0401 a skos:Concept ;
    skos:broader cpg235:0400 ;
    skos:definition "The ability to confirm the origin of data and provide transparency of all alterations." ;
    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 43" ;
    skos:prefLabel "Auditability" .

cpg235:0530 a skos:Concept ;
    skos:broader cpg235:0510 ;
    skos:definition "A regulated entity would normally document data validation processes, including their nature, frequency and level of granularity." ;
    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 56" ;
    skos:prefLabel "Document data validation processes" .

cpg235:0600 a skos:Concept ;
    skos:inScheme cpg235:scheme ;
    skos:prefLabel "Monitoring and management of data issues" ;
    skos:topConceptOf cpg235:scheme .

cpg235:0610 a skos:Concept ;
    skos:broader cpg235:0600 ;
    skos:inScheme cpg235:scheme ;
    skos:prefLabel "Monitoring processes" .

cpg235:0620 a skos:Concept ;
    skos:broader cpg235:0600 ;
    skos:inScheme cpg235:scheme ;
    skos:prefLabel "Data issue management" .

cpg235:0645 a skos:Concept ;
    skos:broader cpg235:0600 ;
