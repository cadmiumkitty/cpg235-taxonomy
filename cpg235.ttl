    skos:inScheme cpg235:scheme ;
    skos:note "CPG 235 - Managing Data Risk s 16(e)" ;
    skos:prefLabel "Availability" .

cpg235:0009 a skos:Concept ;
    skos:broader cpg235:0003 ;
